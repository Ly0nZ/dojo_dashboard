<h1>Listing <%= @dojos.count %> Dojos</h1>

<table>
	<tr>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
	</tr>
	<% @dojos.each do |d| %>
		<tr>
			<td><%= d.branch %></td>
			<td><%= d.street %></td>
			<td><%= d.city %></td>
			<td><%= d.state %></td>
		</tr>
	<% end %>
</table><br>
<br>
<a href="/dojos/new">Add Dojo</a>